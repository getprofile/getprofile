{
  "name": "@getprofile/sdk-js",
  "version": "0.1.0",
  "description": "JavaScript/TypeScript SDK for GetProfile - Build personalized AI experiences with user profiles, traits, and memories",
  "keywords": [
    "getprofile",
    "ai",
    "personalization",
    "user-profiles",
    "memory",
    "traits",
    "sdk",
    "typescript",
    "javascript"
  ],
  "homepage": "https://github.com/getprofile/getprofile#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/getprofile/getprofile.git",
    "directory": "packages/sdk-js"
  },
  "license": "Apache-2.0",
  "author": "GetProfile",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --dts --watch",
    "lint": "eslint . --max-warnings 0",
    "check-types": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/node": "^22.15.3",
    "eslint": "^9.39.1",
    "tsup": "^8.0.0",
    "typescript": "5.9.2",
    "vitest": "^1.0.0"
  },
  "private": false
}
